<Window x:Class="WpfAdmin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Admin - BenchmarkTool" Height="600" Width="900">
    <DockPanel>
        <StackPanel DockPanel.Dock="Left" Width="200" Background="LightGray">
            <Button Content="Home" Click="NavToHome" Margin="5" />
            <Button Content="Bedrijven" Click="NavToBedrijven" Margin="5" />
            <Button Content="Registraties" Click="NavToRegistraties" Margin="5" />
            <Button Content="Uitloggen" Click="NavToLogin" Margin="5" />
        </StackPanel>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" />
    </DockPanel>
</Window>